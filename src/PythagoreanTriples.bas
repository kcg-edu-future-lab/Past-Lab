 10 MAX=100
 20 FOR I=2 TO MAX
 30 FOR J=I+1 TO MAX
 40 K=SQR(I*I+J*J)
 50 IF INT(K)=K THEN GOTO 80
 60 NEXT J
 70 NEXT I
 75 END
 80 M=J
 90 N=I
100 FOR A=1 TO 100
110 R=M-N*INT(M/N)
120 IF R=0 THEN GOTO 200
130 M=N
140 N=R
150 NEXT A
200 IF N=1 THEN PRINT I; "^2 +"; J; "^2 ="; K; "^2"
210 GOTO 60
