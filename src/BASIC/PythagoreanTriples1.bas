 10 MAX=100
 20 FOR A=2 TO MAX-1
 30 FOR B=A+1 TO MAX
 40 C=SQR(A*A+B*B)
 50 IF INT(C)=C THEN GOTO 80
 60 NEXT B
 70 NEXT A
 75 END
 80 M=B
 90 N=A
100 FOR I=1 TO 100
110 R=M-N*INT(M/N)
120 IF R=0 THEN GOTO 200
130 M=N
140 N=R
150 NEXT I
200 IF N=1 THEN PRINT A; "^2 +"; B; "^2 ="; C; "^2"
210 GOTO 60
